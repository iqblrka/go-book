	<queryString>
		<![CDATA[SELECT 
    tj.id_transaksi_jual, 
    tj.id_detail,
    tj.harga_jual, 
    tj.jumlah, 
    tj.tgl_jual, 
    tj.id_barang, 
    b.jenis_barang, 
    b.nama_barang, 
    tj.idMember,
    tj.total_jual, 
    m.nama_member,
    m.diskon 
FROM transaksi_jual tj 
INNER JOIN barang b ON tj.id_barang = b.id_barang 
LEFT JOIN member m ON tj.idMember = m.idMember 
WHERE tj.id_transaksi_jual = $P{id_transaksi_jual}
ORDER BY tj.id_detail]]>
	</queryString>
